"unlitgeneric"
{
	$model 1 	"$basetexture" "unioncity2/miscellaneous/banklaser"
	$additive 1
	$nocull 1
	
	"$noise_alpha" 0.0
	
	"proxies"
    {
        "uniformnoise"
        {
            "minval" 0.75
            "maxval" 1.0
            "resultvar" "$noise_alpha"
        }
	}
}


